package org.td;

public class TwoDimension {
	public static void main(String[] args) {
		int arr[][]=new int[3][3];
		arr[0][0]=20;
		arr[0][1]=20;
		arr[0][2]=20;
		arr[1][0]=20;
		arr[1][1]=20;
		arr[1][2]=20;
		arr[2][0]=20;
		arr[2][1]=20;
		arr[2][2]=20;
		
		System.out.println(arr[0][0]);
		
		for (int i = 0;i<arr.length;i++) {
			for (int j= 0 ;j<arr.length;j++ ) {
				
		System.out.println("the arr is "+arr[i][j]);
				
			
			}
		}
			
		
		
	}

}
